{
  "name": "ic-evm-rpc-gateway",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "EVM_CANISTER_ID=aaaaa-aa tsx tests/run.ts",
    "smoke:viem": "tsx smoke/viem_smoke.ts",
    "smoke:ethers": "tsx smoke/ethers_smoke.ts",
    "smoke:foundry": "bash smoke/foundry_smoke.sh",
    "smoke:all": "bash smoke/run_all.sh"
  },
  "dependencies": {
    "@dfinity/agent": "^3.4.3",
    "@dfinity/candid": "^3.4.3",
    "@dfinity/identity": "^3.4.3",
    "@dfinity/identity-secp256k1": "^3.4.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "ethers": "^6.15.0",
    "viem": "^2.37.13"
  }
}
